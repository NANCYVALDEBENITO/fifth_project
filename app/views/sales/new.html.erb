<h1>Sales#new</h1>
<p>Find me in app/views/sales/new.html.erb</p>


<%= form_with model: @sale do |form| %>
 <%= form.label :cod, "Código" %>
 <%= form.text_field :cod %>
 <%= form.label :detail, "Detalle de la venta" %>
 <%= form.text_field :detail %>
 <%= form.label :category, "Categoría" %>
 <%= form.text_field :category %>
 <%= form.label :value, "Valor" %>
 <%= form.text_field :value %>
 <%= form.label :discount, "Descuento" %>
 <%= form.text_field :discount %>
 <%= form.label :tax, "Exento del IVA" %>
 <%= form.check_box :tax %>
 <%= form.submit "Enviar" %>
<% end %>
